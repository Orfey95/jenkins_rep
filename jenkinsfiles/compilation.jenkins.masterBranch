#!groovy
pipeline {
    agent any  
    tools {
        jdk 'my-jdk'
        maven 'my-maven'
    }
    stages {       	  
	  stage('Artifactory configuration') {
		steps {		        
			rtMavenRun (
			    goals: "clean install",
			    pom: "pom.xml"
			)			
	       } 				  				  
          }
	  stage('Publish to Artifactory') {
            steps {
                rtPublishBuildInfo(serverId: "my-artifactory")
            }
        }
    }	
}
