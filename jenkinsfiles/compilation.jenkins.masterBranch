#!groovy
properties([disableConcurrentBuilds()])
pipeline {
    agent any  
    tools {
        jdk 'my-jdk'
        maven 'my-maven'
    }
    stages {       
	  stage("packaging") {
               steps {
			sh 'mvn package'
	       } 				  				  
          }	
	  stage("sonar analysis") {
               steps {
		        withSonarQubeEnv('my-sonar') {
                            sh 'mvn clean sonar:sonar'
                        }
	       } 				  				  
          }
    }	
}
